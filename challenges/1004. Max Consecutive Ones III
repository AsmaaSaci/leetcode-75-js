var uniqueOccurrences = function(arr) {
    let countMap = new Map();
    for (let num of arr) {
        countMap.set(num, (countMap.get(num) || 0) + 1);
    }
    let counts = new Set();
    for (let val of countMap.values()) {
        if (counts.has(val)) return false;
        counts.add(val);
    }
    return true;
};
